","name":null,"original":{"line":79,"column":0},"generated":{"line":97,"column":30}},{"source":"src/component/teamCard.vue","name":null,"original":{"line":80,"column":0},"generated":{"line":98,"column":0}},{"source":"src/component/teamCard.vue","name":null,"original":{"line":80,"column":0},"generated":{"line":98,"column":9}},{"source":"src/component/teamCard.vue","name":null,"original":{"line":80,"column":0},"generated":{"line":98,"column":21}},{"source":"src/component/teamCard.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":99,"column":0}},{"source":"src/component/teamCard.vue","name":null,"original":{"line":20,"column":0},"generated":{"line":100,"column":0}}],"sources":{"src/component/teamCard.vue":"<template>\r\n  <div class=\"flex flex-col items-center\">\r\n    <p>\r\n      <b>{{ team.name }}</b>\r\n    </p>\r\n    <div class=\"w-auto h-32\">\r\n      <img :src=\"team.imgUrl\" />\r\n    </div>\r\n    <div class=\"bg-white w-full text-center\">\r\n      <p>Winrate {{ getWinrate }}%</p>\r\n      <p>Total Match {{ getTotalmatch }}</p>\r\n      <p>W {{ getTotalWinner }}, L {{ getTotalLose }}</p>\r\n    </div>\r\n    <button class=\"p-2 bg-blue-400 w-full text-white rounded-md\">invite</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"card\",\r\n  data() {\r\n    return {\r\n      histories: [],\r\n    };\r\n  },\r\n  props: [\"team\"],\r\n  methods: {\r\n    getHistories() {\r\n      axios({\r\n        method: \"GET\",\r\n        url: `http://localhost:4000/teams/histories/${this.team.id}`,\r\n      })\r\n        .then(({ data }) => {\r\n          this.histories = data.result;\r\n          console.log(this.histories);\r\n        })\r\n        .catch((err) => console.log(err));\r\n    },\r\n  },\r\n  computed: {\r\n    getWinrate() {\r\n      if (this.getTotalmatch) {\r\n        let winrate = (this.getTotalWinner / this.getTotalmatch) * 100;\r\n        console.log(winrate);\r\n        return String(winrate).slice(0, 5);\r\n      } else {\r\n        return 0;\r\n      }\r\n    },\r\n    getTotalWinner() {\r\n      let winner = this.histories.filter((el) => {\r\n        if (el.theWinnerId == this.team.id) {\r\n          return el;\r\n        }\r\n      });\r\n      if (winner[0]) {\r\n        return winner.length;\r\n      } else {\r\n        return 0;\r\n      }\r\n    },\r\n    getTotalLose() {\r\n      let lose = this.histories.filter((el) => {\r\n        if (el.theWinnerId !== this.team.id) {\r\n          return el;\r\n        }\r\n      });\r\n      if (lose[0]) {\r\n        return lose.length;\r\n      } else {\r\n        return 0;\r\n      }\r\n    },\r\n    getTotalmatch() {\r\n      return this.histories.length;\r\n    },\r\n  },\r\n  created() {\r\n    this.getHistories();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"},"lineCount":null}},"error":null,"hash":"04409226cbe7db70abd361cd2f3e1af4","cacheData":{"env":{}}}